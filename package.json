{
  "name": "coinview",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build:window": "webpack --config webpack.config.js -p",
    "build:common": "babel src --out-dir dist",
    "build": "rm -rf dist/* && npm run build:window && npm run build:common",
    "dev": "webpack --config webpack.config.js -w",
    "start": "npm run serve",
    "test": "npm run lint && npm run test:coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "uuid": "^3.3.2"
  }
}
